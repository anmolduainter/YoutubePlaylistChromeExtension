{
  "manifest_version":2,
  "name":"Budget Manager",
  "version":"1.0",
  "description":"This extension tracks your overall spendings",
  "icons":{
       "128":"smiling.png"
  },
  "browser_action":{
    "default_icon":"smiling.png",
    "default_popup":"popup.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "tabs", "<all_urls>", "http://192.168.0.106/",
    "http://*/*", "https://*/*"
  ]




}